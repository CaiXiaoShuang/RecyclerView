<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.intelligence.recyclerview">

    <application
        android:allowBackup="true"
        android:name="com.intelligence.recyclerview.ExampleApplication"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name="com.intelligence.recyclerview.MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.intelligence.recyclerview.AlphaChangeActivity"
            android:label="@string/app_name" />
        <activity
            android:name="com.intelligence.recyclerview.LinearActivity"
            android:label="@string/app_name" />
        <activity
            android:name="com.intelligence.recyclerview.GridActivity"
            android:label="@string/app_name" />
        <activity
            android:name="com.intelligence.recyclerview.StaggeredGridActivity"
            android:label="@string/app_name" />
        <activity
            android:name="com.intelligence.recyclerview.EmptyViewActivity"
            android:label="@string/app_name" />
        <activity android:name="com.intelligence.recyclerview.CollapsingToolbarLayoutActivity" />
        <activity android:name="com.intelligence.recyclerview.DisableExampleActivity" />
        <activity android:name="com.intelligence.recyclerview.MultiHeaderActivity" />
        <activity android:name="com.intelligence.recyclerview.ItemDecorationActivity" />
    </application>

</manifest>